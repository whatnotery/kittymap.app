
<div id="cats" class="float-left h-full w-1/4 flex-col overflow-auto">
  <%  if current_user.present? %>
    <%= link_to "New cat", new_cat_path, data: { turbo_frame: 'new-cat' }, id: "new-cat-button"%>
    <%= turbo_frame_tag "new-cat" %>
  <% else %>
    <%= link_to "Sign up", new_user_path %> 
    <%= link_to "Sign In", "/sign_in" %>
  <% end %>
  <%= render @cats %>
</div>

<%= js_map Cat.map_data, class: "w-3/4", height: "100vh", tooltips: {html: true, hover: false} %>
 




